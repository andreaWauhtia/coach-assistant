# Environment variables pour le serveur Zapier

# API Key pour l'authentification
COACH_ASSISTANT_API_KEY=your-secret-api-key-change-this-in-production

# Port du serveur
PORT=5000

# Mode debug
DEBUG=false

# Zapier webhooks (vos URLs Zapier personnelles)
ZAPIER_MATCH_WEBHOOK=https://hooks.zapier.com/hooks/catch/YOUR_WEBHOOK_ID
ZAPIER_TRAINING_WEBHOOK=https://hooks.zapier.com/hooks/catch/YOUR_WEBHOOK_ID
ZAPIER_PLAYER_WEBHOOK=https://hooks.zapier.com/hooks/catch/YOUR_WEBHOOK_ID
